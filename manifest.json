{
    "name": "Save4Later",
    "version": "0.1",
    "manifest_version": 2,
    "description": "A list of save-for-later reading",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["popup.js"],
        "persistent": false
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}